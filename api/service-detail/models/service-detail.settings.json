{
  "kind": "collectionType",
  "collectionName": "service_details",
  "info": {
    "name": "Service Detail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "media": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "services": {
      "collection": "services"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "excerpt": {
      "type": "text"
    },
    "all_services_text": {
      "type": "string"
    },
    "quote_text": {
      "type": "string"
    },
    "quote_button": {
      "type": "component",
      "repeatable": false,
      "component": "ui.button"
    }
  }
}
